          </apply>
        </math>
      </assignmentRule>
      <assignmentRule variable="fpg_ratio">
        <math xmlns="http://www.w3.org/1998/Math/MathML">
          <apply>
            <divide/>
            <ci> fpg </ci>
            <ci> fpg0 </ci>
          </apply>
        </math>
      </assignmentRule>
      <assignmentRule variable="fpg_relchange">
        <math xmlns="http://www.w3.org/1998/Math/MathML">
          <apply>
            <divide/>
            <apply>
              <minus/>
              <ci> fpg </ci>
              <ci> fpg0 </ci>
            </apply>
            <ci> fpg0 </ci>
          </apply>
        </math>
      </assignmentRule>
      <assignmentRule variable="hb_total">
        <math xmlns="http://www.w3.org/1998/Math/MathML">
          <apply>
            <plus/>
            <ci> hb </ci>
            <ci> hba1c0 </ci>
          </apply>
        </math>
      </assignmentRule>
      <assignmentRule name="Hill equation for drug effect" variable="E_glp1">
        <notes>
          <body xmlns="http://www.w3.org/1999/xhtml">
            <p>Effect of GLP-1 inhibitor.
Modelled via hill equation.</p>
          </body>
        </notes>
        <math xmlns="http://www.w3.org/1998/Math/MathML">
          <apply>
            <divide/>
            <apply>
              <times/>
              <ci> Emax_glp1 </ci>
              <apply>
                <power/>
                <ci> glp1 </ci>
                <ci> gamma_glp1 </ci>
              </apply>
            </apply>
            <apply>
              <plus/>
              <apply>
                <power/>
                <ci> EC50_glp1 </ci>
                <ci> gamma_glp1 </ci>
              </apply>
              <apply>
                <power/>
                <ci> glp1 </ci>
                <ci> gamma_glp1 </ci>
              </apply>
            </apply>
          </apply>
        </math>
      </assignmentRule>
    </listOfRules>
